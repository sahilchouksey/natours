{
  "name": "natours-1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:css": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss css/style.css -o css/style.prefixed.css",
    "compress:css": "node-sass css/style.prefixed.css css/style.main.css --output-style compressed",
    "build:css": "npm-run-all compile:css concat:css prefix:css compress:css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.0.0"
  },
  "dependencies": {}
}
